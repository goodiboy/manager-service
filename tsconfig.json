{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs", // node不支持module
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
    //    "baseUrl": ".",
    //    "paths": {
    //      "*": ["node_modules/*", "src/*"]
    //    }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
